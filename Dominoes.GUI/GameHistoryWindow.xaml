<Window x:Class="Dominoes.GUI.GameHistoryWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Dominoes.GUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="200">
    <Grid>
        <StackPanel Margin="0,0,0,0" Background="#FFE3EAD0">
            <TextBox x:Name="historyBox" Height="23" TextWrapping="Wrap" Text="History" />
            <DataGrid x:Name="dataGrid"/>

        </StackPanel>
    </Grid>
</Window>
